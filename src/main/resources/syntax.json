[
    {
        "aliases": [
            "json",
            "js"
        ],
        "rules": [
            {
                "match": "/(class|function|const|let|var)/g",
                "apply": "BLUE",
                "priority": 5
            },
            {
                "match": "/([\"'])(?:(?=(\\?))\\2.)*?\\1/g",
                "apply": "LIGHT_RED",
                "priority": 4
            },
            {
                "match": "/(?<!(?:\"|'|\/\/).*)[-+]?[0-9](\\.[0-9]+)?/g",
                "apply": "YELLOW",
                "priority": 3
            },
            {
                "match": "/(true|false)/gi",
                "apply": "LIGHT_GREEN",
                "priority": 2
            },
            {
                "match": "/[{},\\[\\]:]/g",
                "apply": "WHITE",
                "priority": 1
            }
        ]
    }
]